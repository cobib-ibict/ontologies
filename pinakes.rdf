        <rdfs:domain rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#ISSN"/>
        <rdfs:range rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#MeioFisico"/>
        <rdfs:comment xml:lang="pt-br">Indica a relação onde uma entidade possui um suporte ou meio físico associado a ela.</rdfs:comment>
        <rdfs:label xml:lang="pt-br">tem meio físico</rdfs:label>
    </owl:ObjectProperty>
    


