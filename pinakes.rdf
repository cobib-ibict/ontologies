     -->

    


    <!-- http://pinakes.prd.ibict.br/ontology/pinakes#Acre -->

    <owl:NamedIndividual rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Acre">
        <rdf:type rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#UnidadeFederativa"/>
        <rdfs:comment xml:lang="pt-br">Unidade federativa do Brasil representada pela sigla AC</rdfs:comment>
        <rdfs:label xml:lang="pt-br">Acre</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://pinakes.prd.ibict.br/ontology/pinakes#Alagoas -->

    <owl:NamedIndividual rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#Alagoas">
        <rdf:type rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#UnidadeFederativa"/>
        <rdfs:comment xml:lang="pt-br">Unidade federativa do Brasil representada pela sigla AL</rdfs:comment>
