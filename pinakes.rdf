        <owl:inverseOf rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#isSupportOfCollection"/>
        <rdfs:domain rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#Colecao"/>
        <rdfs:range rdf:resource="http://pinakes.prd.ibict.br/ontology/pinakes#MeioFisico"/>
        <rdfs:comment xml:lang="pt-br">Indica a relação em que coleção tem  suporte ou meio físico associados a ela.</rdfs:comment>
        <rdfs:label xml:lang="pt-br">Coleção tem suporte</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://pinakes.prd.ibict.br/ontology/pinakes#hasTitleComplement -->

    <owl:ObjectProperty rdf:about="http://pinakes.prd.ibict.br/ontology/pinakes#hasTitleComplement">
