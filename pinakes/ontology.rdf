    <!-- https://cobib-ibict.github.io/ontologies/pinakes#isPerformedThrough -->

    <owl:ObjectProperty rdf:about="https://cobib-ibict.github.io/ontologies/pinakes#isPerformedThrough">
        <owl:inverseOf rdf:resource="https://cobib-ibict.github.io/ontologies/pinakes#performs"/>
