        <rdfs:isDefinedBy rdf:resource="https://cobib-ibict.github.io/ontologies/pinakes#hasPartAgenteColetivo"/>
        <rdfs:label xml:lang="pt-br">Agente Coletivo possui parte</rdfs:label>
