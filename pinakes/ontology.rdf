    <owl:Ontology rdf:about="https://cobib-ibict.github.io/ontologies/pinakes">
        <dc:rights xml:lang="en">CC BY 4.0</dc:rights>
        <dc:title xml:lang="pt-br">Ontologia Pinakes</dc:title>
        <dc:title xml:lang="en">Pinakes Ontology</dc:title>
        <terms:creator xml:lang="pt-br">Pinakes - COBIB (Ibict)</terms:creator>
        <terms:description xml:lang="pt-br">Desenvolvida a partir dos modelos LRM e PRESSoo para representar, de forma estruturada, os conceitos e as relações do domínio dos serviços bibliográficos (ex, CCN e Bibliodata), modelando descrições de recursos, eventos e vínculos entre entidades usados no acesso à informação bibliográfica.</terms:description>
        <terms:description xml:lang="en">Developed based on the LRM and PRESSoo models to structurally represent the concepts and relationships in the domain of bibliographic services (e.g., CCN and Bibliodata), modeling descriptions of resources, events, and links between entities used in bibliographic information access.</terms:description>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0"/>
        <terms:publisher rdf:resource="https://pinakes.ibict.br"/>
        <doap:bug-database rdf:resource="https://github.com/cobib-ibict/ontologies/issues"/>
        <doap:repository rdf:resource="https://github.com/cobib-ibict/ontologies"/>
        <rdfs:isDefinedBy rdf:resource="https://cobib-ibict.github.io/ontologies/pinakes"/>
        <rdfs:label xml:lang="pt-br">Ontologia Pinakes</rdfs:label>
        <owl:versionInfo xml:lang="pt-br">1.0</owl:versionInfo>
    </owl:Ontology>



