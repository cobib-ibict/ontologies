    <!-- https://cobib-ibict.github.io/ontologies/pinakes#hasPartAgenteColetivo -->

    <owl:ObjectProperty rdf:about="https://cobib-ibict.github.io/ontologies/pinakes#hasPartAgenteColetivo">
